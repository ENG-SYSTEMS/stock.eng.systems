<?xml version='1.0' encoding='utf-8'?>
<widget id="net.keole.stock" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YouPark.me Valet</name>
    <description>
        YouPark.me l'application qui permet aux voituriers de trouver des clients.
    </description>
    <author email="k@keole.net" href="http://www.keole.net">
        ..::Keole dev team
    </author>
    <icon src="www/resources/icons/icon.png"/>
    <content src="index.html" />
    <access origin="*" />
    <access origin="tel:*" launch-external="yes" />
    <access origin="mailto:*" launch-external="yes" />
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.CameraLauncher" />
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="4000" />
    <preference name="SplashMaintainAspectRatio" value="true|false" />
</widget>
